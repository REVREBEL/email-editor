#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/garystringham/github-revrebel/email-editor/node_modules/.pnpm/vue-dts-gen@0.3.0/node_modules/vue-dts-gen/dist/node_modules:/Users/garystringham/github-revrebel/email-editor/node_modules/.pnpm/vue-dts-gen@0.3.0/node_modules/vue-dts-gen/node_modules:/Users/garystringham/github-revrebel/email-editor/node_modules/.pnpm/vue-dts-gen@0.3.0/node_modules:/Users/garystringham/github-revrebel/email-editor/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/garystringham/github-revrebel/email-editor/node_modules/.pnpm/vue-dts-gen@0.3.0/node_modules/vue-dts-gen/dist/node_modules:/Users/garystringham/github-revrebel/email-editor/node_modules/.pnpm/vue-dts-gen@0.3.0/node_modules/vue-dts-gen/node_modules:/Users/garystringham/github-revrebel/email-editor/node_modules/.pnpm/vue-dts-gen@0.3.0/node_modules:/Users/garystringham/github-revrebel/email-editor/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/cli.js" "$@"
else
  exec node  "$basedir/../../dist/cli.js" "$@"
fi
